{
  "semi": true,
  "singleQuote": true,
  "tabWidth": 2,
  "useTabs": false,
  "printWidth": 100,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "arrowParens": "always",
  "endOfLine": "lf",
  "plugins": ["@shopify/prettier-plugin-liquid"],
  "overrides": [{ "files": "*.liquid", "options": { "parser": "liquid-html" } }]
}
